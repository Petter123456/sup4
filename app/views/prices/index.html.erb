<p id="notice"><%= notice %></p>

<h1>Prices</h1>

<table>
  <thead>
    <tr>
      <th>It</th>
      <th>Finance</th>
      <th>Call center</th>
      <th>Logistics</th>
      <th>Marketing</th>
      <th>Technology</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prices.each do |price| %>
      <tr>
        <td><%= price.it %></td>
        <td><%= price.finance %></td>
        <td><%= price.call_center %></td>
        <td><%= price.logistics %></td>
        <td><%= price.marketing %></td>
        <td><%= price.technology %></td>
        <td><%= link_to 'Show', price %></td>
        <td><%= link_to 'Edit', edit_price_path(price) %></td>
        <td><%= link_to 'Destroy', price, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Price', new_price_path %>
