<p id="notice"><%= notice %></p>

<h1>Contracts</h1>

<table>
  <thead>
    <tr>
      <th>Position</th>
      <th>User</th>
      <th>Supplier</th>
      <th>Order spec</th>
      <th>Product area</th>
      <th>Start date</th>
      <th>End date</th>
      <th>City</th>
      <th>Percentage</th>
      <th>Experiance</th>
      <th>Salary</th>
      <th>Supplier</th>
      <th>Supplier price</th>
      <th>Supplier name</th>
      <th>Type of service</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contracts.each do |contract| %>
      <tr>
        <td><%= contract.position %></td>
        <td><%= contract.user_id %></td>
        <td><%= contract.supplier_id %></td>
        <td><%= contract.order_spec_id %></td>
        <td><%= contract.product_area %></td>
        <td><%= contract.start_date %></td>
        <td><%= contract.end_date %></td>
        <td><%= contract.city %></td>
        <td><%= contract.percentage %></td>
        <td><%= contract.experiance %></td>
        <td><%= contract.salary %></td>
        <td><%= contract.supplier %></td>
        <td><%= contract.supplier_price %></td>
        <td><%= contract.supplier_name %></td>
        <td><%= contract.type_of_service %></td>
        <td><%= link_to 'Show', contract %></td>
        <td><%= link_to 'Edit', edit_contract_path(contract) %></td>
        <td><%= link_to 'Destroy', contract, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contract', new_contract_path %>
